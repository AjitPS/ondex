	<name>WS_Test_Client</name>
	<description>Test Webservice Client</description>
	<build>
		<plugins>
			<plugin>
                <dependencies>
                    <dependency>
                        <groupId>apache-xerces</groupId>
                        <artifactId>xercesImpl</artifactId>
                        <version>2.9.1</version>
                    </dependency>
                </dependencies>

