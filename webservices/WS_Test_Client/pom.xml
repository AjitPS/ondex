				<artifactId>cxf-codegen-plugin</artifactId>
				<version>${cxf.version}</version>
				<executions>
					<execution>
						<id>generate-sources-auto</id>
						<phase>generate-sources</phase>
						<configuration>
							<sourceRoot>${project.build.directory}/generated/cxf</sourceRoot>
							<wsdlOptions>
								<wsdlOption>
                                    <wsdl>http://rpc466.cs.man.ac.uk:8080/ondex/services/ondex-auto?wsdl</wsdl>
									<!--wsdl>${project.basedir}/src/main/wsdl/ondex-auto.xml</wsdl-->
									<extraargs>
										<extraarg>-validate</extraarg>
										<extraarg>-client</extraarg>
										<extraarg>-verbose</extraarg>
										<extraarg>-xjc-verbose</extraarg>
									</extraargs>
                                    <packagenames><packagename>net.sourceforge.ondex.webservice.client.auto</packagename></packagenames>
								</wsdlOption>
							</wsdlOptions>
						</configuration>
						<goals>
							<goal>wsdl2java</goal>
						</goals>
					</execution>
					<execution>
						<id>generate-sources-export</id>
